                     {:count 1 :name "Exhaustion"}
                     {:count 2 :name "Preemptive Strike"}
                     {:count 2 :name "Forced Retreat"}
                     {:count 2 :name "Brilliant Plan"}
                     {:count 15 :name "Island"}]}
